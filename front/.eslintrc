{
    "parser": "@typescript-eslint/parser",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "react-app"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-undef": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "overrides": [
        {
            "files": [
                "*.ts",
                "*.tsx"
            ],
            "rules": {
                "@typescript-eslint/no-unused-vars": [
                    2,
                    {
                        "args": "none"
                    }
                ]
            }
        }
    ]
}
